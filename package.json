{
  "name": "@xx1115/cli",
  "version": "0.0.2",
  "scripts": {
    "start": "tsc --watch --project tsconfig-build.json",
    "test": "jest",
    "test:dev": "jest --watch",
    "test:cov": "jest --coverage",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "build:dev": "rimraf dist && tsc -p tsconfig-build.json",
    "dev:cli-init": "ts-node --files -r tsconfig-paths/register ./src/cli/index.ts --help",
    "dev:package": "cross-env NODE_ENV=test ts-node --files -r tsconfig-paths/register ./src/models/package/remote.ts",
    "prepublish": "ts-node scripts/prepublish.ts && npm run build:dev"
  },
  "bin": {
    "xx": "./dist/bin/index.js"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/cli-spinner": "^0.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "^27.0.3",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.11.8",
    "@types/npmlog": "^4.1.3",
    "@types/pacote": "^11.1.1",
    "@types/root-check": "^1.0.0",
    "@types/semver": "^7.3.9",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lerna": "^4.0.0",
    "lint-staged": "^12.0.3",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "chalk": "^4.1.2",
    "cli-spinner": "^0.2.10",
    "commander": "^8.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "import-local": "^3.0.3",
    "inquirer": "^8.2.0",
    "module-alias": "^2.2.2",
    "npmlog": "^6.0.0",
    "pacote": "^12.0.2",
    "rimraf": "^3.0.2",
    "root-check": "^1.0.0",
    "semver": "^7.3.5",
    "simple-git": "^2.47.0",
    "terminal-link": "^2.1.1",
    "uuid": "^8.3.2"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run format"
    ]
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "public": true
  },
  "files": [
    "dist/*",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xx1115/xx-cli.git"
  },
  "bugs": {
    "url": "https://github.com/xx1115/xx-cli/issues"
  },
  "homepage": "https://github.com/xx1115/xx-cli#readme",
  "keywords": [
    "cli",
    "init",
    "publish",
    "commit"
  ],
  "author": "Marvin <huo_marvin@163.com>",
  "license": "MIT"
}
